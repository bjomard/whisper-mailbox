# Ethereum RPC (mainnet for ENS)
RPC_URL=https://eth-mainnet.g.alchemy.com/v2/REPLACE_ME

# Signer used by F3NIX to create subdomains and publish records
SIGNER_PRIVATE_KEY=0xREPLACE_ME

# ENS root name you own
ENS_ROOT=f3nixid.eth

# PublicResolver address (mainnet commonly uses this, but you SHOULD confirm per network)
# Mainnet PublicResolver (2025-era) often: 0x226159d592E2b063810a10Ebf6dcbADA94Ed68b8
PUBLIC_RESOLVER=0x226159d592E2b063810a10Ebf6dcbADA94Ed68b8

# Base URL where profiles are hosted
PROFILE_BASE_URL=https://profiles.f3nix.net/wspr
